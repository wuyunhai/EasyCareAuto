<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!--log 日志记录-->
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
    <!--SocketEngine-->
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <!--服务信息描述，在window服务模式下的名称标识-->
  <appSettings>
    <add key="ServiceName" value="MES.SocketService" />
    <add key="ServiceDescription" value="MES与EQC通讯处理程序" />
  
    <add key="GXH_4_DeviceID" value="L01.IN.OP004.01" /><!--个性化线_个性化灌装设备ID-->
    <add key="GXH_5_DeviceID" value="L01.IN.OP005.01" /><!--个性化线_搅拌设备ID-->
    <add key="GXH_6_DeviceID" value="L01.IN.OP006.01" /><!--个性化线_称重设备ID-->
    <add key="GXH_7_DeviceID" value="L01.IN.OP007.01" /><!--个性化线_旋盖设备ID-->
    <add key="GXH_8_DeviceID" value="L01.IN.OP008.01" /><!--个性化线_分流设备ID-->
    <add key="GXH_9_DeviceID" value="L01.IN.OP009.01" /><!--个性化线_仓库分配设备ID-->
    <add key="GXH_10_DeviceID" value="L01.IN.OP010.01" /><!--个性化线_自动入库设备ID-->

    <add key="CH_1_DeviceID" value="L01.OP001.01" /><!--彩盒线_上彩盒_设备ID--> 
    <add key="CH_2_DeviceID" value="L01.OP002.01" /><!--彩盒线_通用料_设备ID--> 
    <add key="CH_3_DeviceID" value="L01.OP003.01" /><!--彩盒线_齐套_设备ID-->
     
  </appSettings>
   
  <!--name: 实例名称 命名空间
      serverType: 实例运行的AppServer类型
      ip: 侦听ip
      port: 侦听端口-->
  <superSocket xmlns="http://schema.supersocket.net/supersocket" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://schema.supersocket.net/supersocket http://schema.supersocket.net/v1-6/supersocket.xsd"
               logFactory="ConsoleLogFactory"
               disablePerformanceDataCollector="true" 
               maxWorkingThreads="5000" 
               maxCompletionPortThreads="5000" 
               minWorkingThreads="5" 
               minCompletionPortThreads="5" 
               isolation="None">
    <servers>
      <!--textEncoding 编码方式"gb2312","utf-8" 默认是acii-->
      <server 
        name="MES.SocketService" 
        serverType="MES.SocketService.MesServer,MES.SocketService" 
        ip="Any" 
        port="6543" 
        mode="Tcp" 
        syncSend="true" 
        clearIdleSession="false" 
        clearIdleSessionInterval="60" 
        idleSessionTimeOut="60" 
        sendBufferSize="65535" 
        receiveBufferSize="65535" 
        maxRequestLength="65535" 
        maxCommandLength="65535" 
        maxConnectionNumber="5000" 
        textEncoding="UTF-8">
      </server>
    </servers>
    <logFactories>
      <add name="ConsoleLogFactory" 
           type="SuperSocket.SocketBase.Logging.ConsoleLogFactory, SuperSocket.SocketBase" />
    </logFactories>
  </superSocket>

  <runtime>
    <gcServer enabled="true" />
  </runtime>
</configuration>